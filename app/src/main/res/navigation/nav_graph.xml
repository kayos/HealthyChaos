<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.kayos.healthykayos.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_RecordingsFragment"
            app:destination="@id/RecordingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/RecordingsFragment"
        android:name="com.kayos.healthykayos.RecordingsFragment"
        android:label="@string/recordings_fragment_label"
        tools:layout="@layout/fragment_recordings">

        <action
            android:id="@+id/action_RecordingsFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
</navigation>